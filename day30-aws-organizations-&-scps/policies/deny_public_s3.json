{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyS3PublicAclsAndPolicy",
      "Effect": "Deny",
      "Action": [
        "s3:PutBucketPublicAccessBlock",
        "s3:PutBucketAcl",
        "s3:PutBucketPolicy"
      ],
      "Resource": "*",
      "Condition": {
        "Bool": {
          "s3:ExistingObjectPublic": "true"
        }
      }
    }
  ]
}